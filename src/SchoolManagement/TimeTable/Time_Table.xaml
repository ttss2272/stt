<Window x:Class="SchoolManagement.TimeTable.Time_Table"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Time_Table" Height="590" Width="1014" xmlns:my="clr-namespace:SchoolManagement" Loaded="Window_Loaded">
    <Grid>
        <StackPanel>
            <my:Menu_UserControl HorizontalAlignment="Left" x:Name="menu_UserControl1" VerticalAlignment="Top" />
        </StackPanel>
        <GroupBox Header="" Height="100" HorizontalAlignment="Left" Margin="229,38,0,0" Name="groupBox1" VerticalAlignment="Top" Width="600">
            <Grid>
                <ComboBox Height="23" ItemsSource="{Binding}" Margin="170,13,240,41" Name="cbBranchName" Width="178" />
                <Label Content="Branch Name" Height="28" HorizontalAlignment="Left" Margin="77,10,0,0" Name="label1" VerticalAlignment="Top" />
                <Button Content="Go" FontWeight="Bold" Height="28" HorizontalAlignment="Left" Margin="388,9,0,0" Name="btnGo" VerticalAlignment="Top" Width="47" />
                <RadioButton Content="Class Wise View" Height="16" HorizontalAlignment="Left" Margin="81,51,0,0" Name="radioButton1" VerticalAlignment="Top" />
                <RadioButton Content="Room Wise View" Height="20" HorizontalAlignment="Left" Margin="217,51,0,0" Name="radioButton2" VerticalAlignment="Top" />
                <RadioButton Content="Teacher Wise View" Height="16" HorizontalAlignment="Left" Margin="357,52,0,0" Name="radioButton3" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <GroupBox Header="" Height="160" HorizontalAlignment="Left" Margin="229,138,0,0" Name="groupBox2" VerticalAlignment="Top" Width="600">
            <Grid>
                <Label Content="Class Name" Height="28" HorizontalAlignment="Left" Margin="7,5,0,0" Name="label3" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="105,8,0,0" Name="cbClassName" VerticalAlignment="Top" Width="160" />
                <Label Content="*" FontWeight="Bold" Foreground="Red" Height="20" HorizontalAlignment="Left" Margin="84,8,0,0" Name="label4" VerticalAlignment="Top" />
                <Label Content="Batch Name" Height="28" HorizontalAlignment="Left" Margin="7,48,0,0" Name="label5" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="105,51,0,0" Name="cbBatchName" VerticalAlignment="Top" Width="160" />
                <Label Content="*" FontWeight="Bold" Foreground="Red" Height="20" HorizontalAlignment="Left" Margin="84,51,0,0" Name="label6" VerticalAlignment="Top" />
                <Label Content="Room Name" Height="28" HorizontalAlignment="Left" Margin="6,90,0,0" Name="label8" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="105,93,0,0" Name="cbRoomName" VerticalAlignment="Top" Width="160" />
                <Label Content="*" FontWeight="Bold" Foreground="Red" Height="20" HorizontalAlignment="Left" Margin="84,93,0,0" Name="label9" VerticalAlignment="Top" />
                <Label Content="Subject Name" Height="28" HorizontalAlignment="Left" Margin="309,8,0,0" Name="label10" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="419,11,0,0" Name="cbSubjectName" VerticalAlignment="Top" Width="160" />
                <Label Content="*" FontWeight="Bold" Foreground="Red" Height="20" HorizontalAlignment="Left" Margin="398,11,0,0" Name="label11" VerticalAlignment="Top" />
                <Label Content="Teacher Name" Height="28" HorizontalAlignment="Left" Margin="309,50,0,0" Name="label12" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="419,53,0,0" Name="cbTeacherName" VerticalAlignment="Top" Width="160" />
                <Label Content="*" FontWeight="Bold" Foreground="Red" Height="20" HorizontalAlignment="Left" Margin="398,53,0,0" Name="label13" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <GroupBox Header="" Height="80" HorizontalAlignment="Left" Margin="229,298,0,0" Name="groupBox3" VerticalAlignment="Top" Width="600">
            <Grid>
                <Button Content="Save" FontWeight="Bold" Foreground="Black" Height="31" HorizontalAlignment="Left" Margin="130,10,0,0" Name="btnSave" TabIndex="14" VerticalAlignment="Top" Width="80" Click="btnSave_Click" />
                <Button Content="Delete" FontWeight="Bold" Foreground="Black" Height="31" HorizontalAlignment="Left" Margin="261,10,0,0" Name="btnDelete" TabIndex="16" VerticalAlignment="Top" Width="75" />
                <Button Content="Clear" FontWeight="Bold" Foreground="Black" Height="31" HorizontalAlignment="Left" Margin="381,10,0,0" Name="btnclear" TabIndex="17" VerticalAlignment="Top" Width="75" />
            </Grid>
        </GroupBox>
        <DataGrid AutoGenerateColumns="False" Height="155" HorizontalAlignment="Left" Margin="235,390,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="600" />
    </Grid>
</Window>
